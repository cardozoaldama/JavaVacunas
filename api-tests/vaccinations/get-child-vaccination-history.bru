meta {
  name: Get Child Vaccination History
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/vaccinations/child/{{testChildId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{parentToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return array of vaccination records", function() {
    const data = res.getBody();
    expect(data).to.be.an("array");
  });

  test("records should have required properties", function() {
    const data = res.getBody();
    if (data.length > 0) {
      expect(data[0]).to.have.property("id");
      expect(data[0]).to.have.property("vaccineId");
      expect(data[0]).to.have.property("doseNumber");
    }
  });
}
