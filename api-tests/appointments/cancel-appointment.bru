meta {
  name: Cancel Appointment
  type: http
  seq: 8
}

put {
  url: {{baseUrl}}/appointments/{{testAppointmentId}}/cancel
  body: none
  auth: bearer
}

auth:bearer {
  token: {{doctorToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should have CANCELLED status", function() {
    const data = res.getBody();
    expect(data.status).to.equal("CANCELLED");
  });
}
