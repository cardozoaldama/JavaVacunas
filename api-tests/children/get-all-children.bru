meta {
  name: Get All Children
  type: http
  seq: 4
}

get {
  url: {{baseUrl}}/children
  body: none
  auth: bearer
}

auth:bearer {
  token: {{doctorToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return array of children", function() {
    const data = res.getBody();
    expect(data).to.be.an("array");
  });

  test("children should have required properties", function() {
    const data = res.getBody();
    if (data.length > 0) {
      expect(data[0]).to.have.property("id");
      expect(data[0]).to.have.property("firstName");
      expect(data[0]).to.have.property("lastName");
      expect(data[0]).to.have.property("documentNumber");
    }
  });
}
