meta {
  name: Get All Active Vaccines
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/vaccines
  body: none
  auth: bearer
}

auth:bearer {
  token: {{doctorToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return array of vaccines", function() {
    const data = res.getBody();
    expect(data).to.be.an("array");
  });

  test("vaccines should have required properties", function() {
    const data = res.getBody();
    if (data.length > 0) {
      expect(data[0]).to.have.property("id");
      expect(data[0]).to.have.property("name");
      expect(data[0]).to.have.property("diseasePrevented");
    }
  });
}
