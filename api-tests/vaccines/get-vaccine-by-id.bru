meta {
  name: Get Vaccine by ID
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/vaccines/{{testVaccineId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{doctorToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return vaccine object", function() {
    const data = res.getBody();
    expect(data).to.be.an("object");
    expect(data).to.have.property("id");
    expect(data.id).to.equal(parseInt(bru.getEnvVar("testVaccineId")));
  });
}
