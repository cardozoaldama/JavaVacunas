meta {
  name: Update Inventory Quantity
  type: http
  seq: 5
}

put {
  url: {{baseUrl}}/inventory/{{testInventoryId}}/quantity?quantity=450
  body: none
  auth: bearer
}

auth:bearer {
  token: {{doctorToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return updated inventory", function() {
    const data = res.getBody();
    expect(data.quantity).to.equal(450);
  });
}
