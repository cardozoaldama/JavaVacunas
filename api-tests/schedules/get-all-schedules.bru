meta {
  name: Get All Vaccination Schedules
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/schedules
  body: none
  auth: bearer
}

auth:bearer {
  token: {{parentToken}}
}

tests {
  test("should return 200 status", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return array of schedules", function() {
    const data = res.getBody();
    expect(data).to.be.an("array");
  });

  test("schedules should have required properties", function() {
    const data = res.getBody();
    if (data.length > 0) {
      expect(data[0]).to.have.property("id");
      expect(data[0]).to.have.property("ageInMonths");
    }
  });
}
